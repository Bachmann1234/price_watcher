<div id="scaqv-metadata"
autoconfig = "yes"
noimage = "//cdn.shopify.com/s/files/1/1706/8129/t/70/assets/sca-qv-no-image.jpg?v=10411977357100053358"
moneyFormat = '${{amount}}'
