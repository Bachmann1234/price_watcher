<script type='text/javascript'>
P.when('A','jQuery').execute(function(A,$) {
    P.now('vas-logger','A','jQuery').execute(function(vasLogger,A,$) {
        if(vasLogger == undefined) {
            P.when('A','jQuery').register('vas-logger',function(A,$){
                return {
                    ueMetricIncreaseCount : function(metricName) {
                        if (typeof ue !== 'undefined') {
                            if(typeof ue.count !== 'undefined')
                                ue.count(metricName,1);
                        }
                    },
                    ueMetricIncreaseCountBy : function(metricName, value) {
                        if (typeof ue !== 'undefined') {
                            if(typeof ue.count !== 'undefined')
                                ue.count(metricName,value);
                        }
                    },
                    refTag : function(refTag) {
                        $.get('/gp/ls/impress.html/ref=' + refTag);
                    }
                }
            });
        }
    });
});
</script>

<script type = 'text/javascript'>
    P.when('A', 'ready').register('vas-mason-metric-publisher', function(A) {
        var $ = A.$;

        return {
            /**
             * [getCSMName used to get a metric name]
             *
             * @param  {[String]} tenantName [tenant associated with the metric being recorded ex: vas, sts]
             * @param  {[String]} widgetName [widget associated with the metric being recorded ex: fakeTwister, PPDBundles]
             * @param  {[String]} actionName [action associated with the metric being recorded ex: gv, serviceSelected]
             *
             */
            getCSMName : function(tenantName, widgetName, actionName){
                var metricName ="";
                if (tenantName && widgetName && actionName) {
                    metricName = (tenantName + '_' + widgetName + '_' + actionName);
                }
                return metricName;
            },

            /**
             * [logCSMCounter used to record CSMs for any event like success, failure, widget load etc]
             * @param  {[String]} ueMetricName [metric name send by client]
             */
            logCSMCounter : function(ueMetricName){
                if (ueMetricName) {
                    if (typeof ue !== 'undefined') {
                        if (typeof ue.count !== 'undefined') {
                            ue.count(ueMetricName, (ue.count(ueMetricName) || 0) + 1);
                        }
                    }
                }
            },

            /**
             * [increaseCSMCounter By used to increase the metric count like twister change, addtocart etc]
             * @param  {[String]} ueMetricName [metric name send by client]
             * @param  {[Number]} value      [what value we should increase]
             */
            increaseCSMCounterBy : function(ueMetricName, value){
                if (ueMetricName) {
                    if (typeof ue !== 'undefined') {
                        if (typeof ue.count !== 'undefined') {
                            ue.count(ueMetricName, (ue.count(ueMetricName) || 0) + value);
                        }
                    }
                }
            },

            /**
             * [getRefMarker used to get a refmarker]
             *
             * @param  {[String]} tenantName [tenant associated with the refmarker being recorded ex: vas]
             * @param  {[String]} mappingSet [mappingSet associated with the refmarker being recorded ex: ahs, hb]
             * @param  {[String]} widgetName [widget associated with the refmarker being recorded ex: fakeTwister, PPDBundles]
             * @param  {[String]} actionName [action associated with the refmarker being recorded ex: gv, serviceSelected]
             *
             */
            getRefMarker : function(tenantName, mappingSet, widgetName, actionName) {
                var refMarker ="";
                if (tenantName && mappingSet && widgetName && actionName) {
                    refMarker = (tenantName + '_' + mappingSet + '_' + widgetName + '_' + actionName);
                }
                return refMarker;
            },

            /**
             * [logHit used to log a hit]
             * @param  {[String]} refMarker           [refmarker associated with the hit]
             * @param  {[Object]} hitParameters       [parameters associated for logging a hit]
             */
            logHit : function(refMarker, hitParameters){
                if(refMarker && refMarker.length <= 32) {
                    var prefix = "?";
                    var refTagParams = "";
                    if (hitParameters) {
                        if (hitParameters.additionalRequestData) {
                            refTagParams += (prefix + "additionalRequestData=" + JSON.stringify(hitParameters.additionalRequestData));
                            prefix = "&";
                        }
                        if (hitParameters.productAsin) {
                            refTagParams += (prefix + "pageAsin=" + hitParameters.productAsin);
                            prefix = "&";
                        }
                        if (hitParameters.upsellAsin) {
                            refTagParams += (prefix + "pageTypeId=" + hitParameters.upsellAsin);
                            prefix = "&";
                        }
                        if (hitParameters.pageType) {
                            refTagParams += (prefix + "pageType=" + hitParameters.pageType);
                            prefix = "&";
                        }
                        if (hitParameters.subPageType) {
                            refTagParams += (prefix + "subPageType=" + hitParameters.subPageType);
                            prefix = "&";
                        }
                        if (hitParameters.hitType) {
                            refTagParams += (prefix + "hitType=" + hitParameters.hitType);
                            prefix = "&";
                        } else {
                            refTagParams += (prefix + "hitType=popUp");
                            prefix = "&";
                        }
                        if (hitParameters.merchantId) {
                            refTagParams += (prefix + "merchantId=" + hitParameters.merchantId);
                            prefix = "&";
                        }
                        if (hitParameters.upsellAsinPrice) {
                            refTagParams += (prefix + "sdpMinimumPriceNew=" + hitParameters.upsellAsinPrice);
                            prefix = "&";
                        }
                        if (hitParameters.productAsinPrice) {
                            refTagParams += (prefix + "merchantAsinPrice=" + hitParameters.productAsinPrice);
                            prefix = "&";
                        }
                    }

                    $.get('/gp/ls/impress.html/ref=' + refMarker + refTagParams);
                }
            },

            hitParameterKeys: {
                upsellAsin : 'upsellAsin',
                productAsin: 'productAsin',
                pageType: 'pageType',
                subPageType: 'subPageType',
                upsellAsinPrice: 'upsellAsinPrice',
                productAsinPrice: 'productAsinPrice',
                hitType: 'hitType',
                additionalRequestData: 'additionalRequestData',
                merchantId: 'merchantId'
            },

            CSMActionTypes : {
                 glanceView : 'gv',
                 addToCart : 'atc',
                 serviceIncluded : 'serviceIncluded',
                 serviceNotIncluded : 'serviceNotIncluded',
                 expandBulletPoints : 'seeMore',
                 collapseBulletPoints : 'seeLess',
                 redirect : 'SDPRedirect',
                 addInstallation : 'addInstallation',
                 skip : 'skip',
                 escape: 'escape'
            },

            RefmarkerActionTypes : {
                 glanceView : 'gv',
                 addToCart : 'att',
                 serviceIncluded : 'incl',
                 serviceNotIncluded : 'cncl',
                 expandBulletPoints : 'expbp',
                 collapseBulletPoints : 'colbp',
                 redirect : 'redir'
            }
        };
    });
</script>



        
    
    


    
    
    
    
  
  


    


    
    
    
    
    
    
    
    
    
    
    
    
    
      
      
    
        


  
    
    
    
    
    
    
    
    
    
    
    
    



    





    
    
    
    
    
   
        
    
  
  
    

  












